{
	"_version": "1.7.0",
	"sap.app": {
		"id": "OptarosSAPUI5POC",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"

		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "OptarosSAPUI5POC.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "OptarosSAPUI5POC.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		  "routing": {
            "config": {
                "viewPath": "OptarosSAPUI5POC.view",
                "controlId": "app",
                "viewLevel": 1,
                "controlAggregation": "pages",
                "routerClass": "sap.m.routing.Router"
            },
            "targets": {
                "target_MainPage": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearAggregation": true,
                    "viewName": "MainPage",
                    "viewLevel": 1
                },
                "target_Detail": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearAggregation": true,
                    "viewName": "CallDetail",
                    "viewLevel": 2
                }
            },
            "routes": [
                {
                    "name": "target_MainPage",
                    "pattern": "",
                    "titleTarget": "",
                    "greedy": false,
                    "target": [
                        "target_MainPage"
                    ]
                },
                {
                    "name": "target_Detail",
                    "pattern": "Detail",
                    "titleTarget": "",
                    "greedy": false,
                    "target": [
                        "target_Detail"
                    ]
                }
            ]
        }
	}
}